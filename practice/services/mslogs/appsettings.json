{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "KafkaSettings": {
    "BootstrapServers": "kafka:9092",
    "Topic": "user-events"
  },
  "MongoDBSettings": {
    "ConnectionString": "mongodb://mongodb-logs:27017",
    "DatabaseName": "LogsDb",
    "UsersCollectionName": "Logs"
  }
}
